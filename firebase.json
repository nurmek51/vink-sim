{
  "hosting": {
    "public": "web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/auth/verify**",
        "destination": "/auth-handler.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "flex-travel-sim-aa",
          "appId": "1:334257707708:android:1a2c9ac7e4b40006500eb9",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "flex-travel-sim-aa",
          "appId": "1:334257707708:ios:b83fac8d3ef998ac500eb9",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "flex-travel-sim-aa",
          "configurations": {
            "android": "1:334257707708:android:1a2c9ac7e4b40006500eb9",
            "ios": "1:334257707708:ios:b83fac8d3ef998ac500eb9",
            "web": "1:334257707708:web:2556c473cd55b681500eb9"
          }
        }
      }
    }
  }
}